    "ui": "npx tsc && node build/server.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "express": "^4.21.0",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.8",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.8.2"
  }
