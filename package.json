{
  "name": "mcp-orchestrator",
  "version": "0.1.0",
  "type": "module",
  "description": "Connect MCPs locally and automate actions between them",
  "main": "build/index.js",
  "bin": {
    "mcp-orchestrator": "./build/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node build/index.js",
